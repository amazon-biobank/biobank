{
    "name": "currency",
    "version": "24.0.0",
    "description": "Biobank contract implemented in JavaScript - channel 2",
    "main": "index.js",
    "engines": {
        "node": ">=15.6",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive",
        "start": "fabric-chaincode-node start"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "dinero.js": "^1.8.1",
        "fabric-contract-api": "^2.0.0",
        "fabric-shim": "^2.0.0",
        "jsrsasign": "^8.0.24",
        "node": "^15.12.0",
        "npm-check-updates": "^11.3.0"
    },
    "devDependencies": {
        "chai": "^4.1.2",
        "eslint": "^4.19.1",
        "fabric-network": "^2.2.0",
        "mocha": "^5.2.0",
        "nyc": "^14.1.1",
        "sinon": "^6.0.0",
        "sinon-chai": "^3.2.0"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
