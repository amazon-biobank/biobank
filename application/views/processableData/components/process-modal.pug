.modal.fade(id= "processModal"+index tabindex='-1' role='dialog' aria-labelledby='exampleModalLabel' aria-hidden='true')
  .modal-dialog(role='document')
    .modal-content
      .modal-header
        h5#exampleModalLabel.modal-title Processar esse dado?
        button.close(type='button' data-dismiss='modal' aria-label='Close')
          span(aria-hidden='true') &times;
      .modal-body 
        form.user(action='/process-request/create' method='post')
          input#data-id(hidden name='raw_data_id' type='text' value=data.id)
          input#collector(hidden name='collector' type='text' value=data.collector)
          input#type(hidden name='type' type='text' value='process')
          .form-group.mb-4
            label(for='processor_id') Id do Processador
            input#processor-id.form-control.form-control-user(name='processor_id' type='text')

          .mb-3
            | Clique em &quot;Processar&quot; abaixo para confirmar a solicitação de processamento

          .modal-footer
            button.btn.btn-secondary(type='button' data-dismiss='modal') Cancelar
            input.btn.btn-primary.btn-user(type='submit' value='Processar')